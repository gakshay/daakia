<%= form_for("user", :url => user_session_path, :class=>"form-inline") do |f| %>
  <%= f.text_field :mobile, :class => "input-small", :placeholder => "Mobile Number", :maxlength => "13"%>
  <%= f.password_field :password, :class => "input-mini", :maxlength => "6", :placeholder => "Password" %>
  <%= f.submit 'Login', :class => "btn btn-danger" %>
  <%#= link_to "Forgot your password?", new_password_path('user') %>
<% end %>